using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SoloLearn
{
    class Program
    {
        static void Main(string[] args)
        {
            int eggsInTotal = Convert.ToInt32(Console.ReadLine()); 
            int eggsIFound = Convert.ToInt32(Console.ReadLine()); 
            int eggsFriendFound = Convert.ToInt32(Console.ReadLine());
            
            if (eggsIFound + eggsFriendFound == eggsInTotal)
            {
                Console.Write("Candy Time");
            }
            else 
            {
                Console.Write("Keep Hunting");
            }
        }
    }
} 
