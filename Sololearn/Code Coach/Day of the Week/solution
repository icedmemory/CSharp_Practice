using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SoloLearn
{
    class Program
    {
        static void Main(string[] args)
        {
            string date = Console.ReadLine();

            DateTime weekday = DateTime.Parse(date);

            Console.Write(weekday.ToString("dddd"));
        }
    }
}
