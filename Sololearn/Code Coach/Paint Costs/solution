using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SoloLearn
{
    class Program
    {
        static void Main(string[] args)
        {
            int colors = Convert.ToInt32(Console.ReadLine());

            double price = (40 + colors * 5) * 1.1;

            Console.WriteLine(Convert.ToInt32(price));
        }
    }
} 
