using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SoloLearn
{
    class Program
    {
        static void Main(string[] args)
        {
            int myAge = Convert.ToInt32(Console.ReadLine()); 
            int yearsPassed = Convert.ToInt32(Console.ReadLine());

            int myTwinAge = myAge + yearsPassed;
            
            Console.Write($"My twin is {myTwinAge} years old and they are {yearsPassed} years older than me");
        }
    }
} 
